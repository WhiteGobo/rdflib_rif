Document(
  Prefix(ex <http://example.com/example#>)
  Prefix(rdf <http://www.w3.org/1999/02/22-rdf-syntax-ns#>)

  Group (
    Forall ?obj ?transObj such that ?obj[rdf:type -> ex:in](
	  Do ( (?transObj New())
		  Assert( ?obj[ex:generated -> ?transObj] )
		  Assert( ?transObj[rdf:type -> ex:out] )
	  )
    )

    ex:A[rdf:type -> ex:in]
  )
)
